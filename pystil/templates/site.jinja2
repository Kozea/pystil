{% extends "_layout.jinja2" %}

{% block article %}
  <article id="graphs">
    <aside class="last-visits"></aside>
    <aside class="map"></aside>
    <aside id="day" class="graph line"></aside>
    <aside id="hour" class="graph bar"></aside>
    {% if site == '*' %}
      <aside id="host" class="graph pie"></aside>
    {% else %}
      <aside id="page" class="graph pie"></aside>
      <aside id="hash" class="graph pie"></aside>
      <aside id="query" class="graph pie"></aside>
    {% endif %}
    <aside id="browser_name" class="graph pie"></aside>
    <aside id="browser_with_version" class="graph pie"></aside>
    <aside id="platform" class="graph pie"></aside>
    <aside id="country" class="graph pie"></aside>
    <aside id="simple_referrer" class="graph pie"></aside>
    <aside id="size" class="graph pie"></aside>
    <aside id="time" class="graph bar"></aside>
  </article>
{% endblock article %}
