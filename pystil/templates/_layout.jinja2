<!DOCTYPE html>
<html>
  <head>
    <title>pystil - An elegant website traffic analyzer</title>
    {% block script %}
      <!--[if lt IE 9]><script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
      <!--[if lte IE 8]><script src="{{ static_url('js/excanvas.min.js') }}"></script><![endif]-->
      <script type="text/javascript">
        (function() {
        var d=document,t='script',p=d.createElement(t),s=d.getElementsByTagName(t)[0];
        p.type='text/javascript';p.async=true;p.src="/pystil.js";s.parentNode.insertBefore(p,s);
        })();
      </script>
      <script src="{{ static_url('js/js.js') }}"></script>
    {% end %}

    {% block style %}
      <link rel="stylesheet" href="http://code.jquery.com/ui/1.8.16/themes/ui-lightness/jquery-ui.css" type="text/css" />
      <link rel="stylesheet" href="{{ static_url('css.css') }}" type="text/css" />
    {% end %}
  </head>
  <body>
    <header>
      <h1>
        <a href="/">Pystil</a>
      </h1>
    </header>
    <nav>
      <ul>
        <li><a href="{{ reverse_url('Index') }}">Home</a></li>
        <li><a href="{{ reverse_url('Sites') }}">Sites</a></li>
        {# <li><a href="{{ url_for('keys') }}">Secret keys</a></li> #}
      </ul>
    </nav>
    <section>
      {% block article %}
      {% end %}
    </section>
    <footer>
      <ul>
        <li><a href="http://community.kozea.org/">A Kozea Project</a></li>
        <li><a href="http://validator.w3.org/check?uri=referer">Optimized for Standards</a></li>
      </ul>
    </footer>
  </body>
</html>
