<table>
  <thead>
    <tr>
      <th>Visits</th>
      <th>Domain</th>
      <th>Stats</th>
    </tr>
  </thead>
  <tbody>
    {% for site in sites %}
      {% if site and site.host %}
        <tr>
          <td>{{ site.count }}</td>
          <td>{{ site.host }}</td>
          <td class="edit"><a href="{{ reverse_url('Site', site.host) }}">Go</a></td>
        </tr>
      {% end %}
    {% end %}
  </tbody>
</table>
